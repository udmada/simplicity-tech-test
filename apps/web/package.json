{
  "name": "@udmada/web",
  "description": "Finance demo web app (React Router + Cloudflare Workers).",
  "private": true,
  "cloudflare": {
    "label": "React Router Framework Starter",
    "products": [
      "Workers"
    ],
    "categories": [
      "storage"
    ],
    "preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/36a99c4a-8919-4e6e-ac9e-e55974407200/public",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/65455e96-7992-425f-44e4-30f3e7d7f900/preview",
    "publish": true
  },
  "dependencies": {
    "@udmada/finance-client": "workspace:*",
    "@udmada/finance-powertools": "workspace:*",
    "effect": "^3.19.2",
    "isbot": "^5.1.32",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "7.9.5"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.14.0",
    "@cloudflare/workers-types": "4.20251106.1",
    "@react-router/dev": "7.9.5",
    "@tailwindcss/vite": "4.1.17",
    "@types/node": "24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "4.46.0"
  },
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "lint": "pnpm exec eslint app --ext .ts,.tsx",
    "format": "pnpm exec prettier \"app/**/*.{ts,tsx}\" --write",
    "format:check": "pnpm exec prettier \"app/**/*.{ts,tsx}\" --check",
    "preview": "react-router build && vite preview",
    "typecheck": "pnpm run cf-typegen && tsc -b",
    "cf-typegen": "wrangler types && react-router typegen",
    "deploy": "wrangler deploy"
  },
  "type": "module"
}
