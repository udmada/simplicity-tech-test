{
  "name": "@udmada/finance-client",
  "version": "0.1.0",
  "type": "module",
  "description": "Personal finance client with DDD, Event-Driven architecture, and Effect-TS",
  "keywords": [
    "finance",
    "plaid",
    "open-banking",
    "effect-ts",
    "ddd",
    "event-driven"
  ],
  "author": "Adam Du",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./effect": {
      "types": "./dist/effect/index.d.ts",
      "import": "./dist/effect/index.js",
      "require": "./dist/effect/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "vitest run --typecheck.only",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm exec eslint src tests --ext .ts",
    "format": "pnpm exec prettier --write src tests",
    "format:check": "pnpm exec prettier --check src tests"
  },
  "dependencies": {
    "effect": "^3.10.14",
    "plaid": "^27.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5",
    "@vitest/ui": "^2.1.5"
  },
  "peerDependencies": {
    "effect": "^3.0.0"
  }
}
