{
  "name": "@udmada/finance-client",
  "version": "0.1.0",
  "type": "module",
  "description": "Personal finance client with DDD, Event-Driven architecture, and Effect",
  "keywords": [
    "finance",
    "plaid",
    "open-banking",
    "effect",
    "DDD",
    "event-driven"
  ],
  "author": "udmada <git@adamdu.nz>",
  "license": "MIT",
  "homepage": "https://github.com/udmada/simplicity-tech-test/tree/master/packages/finance-client#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udmada/simplicity-tech-test.git",
    "directory": "packages/finance-client"
  },
  "bugs": {
    "url": "https://github.com/udmada/simplicity-tech-test/issues"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./effect": {
      "types": "./dist/esm/effect/index.d.ts",
      "import": "./dist/esm/effect/index.js",
      "require": "./dist/cjs/effect/index.js"
    },
    "./infrastructure": {
      "types": "./dist/esm/infrastructure/index.d.ts",
      "import": "./dist/esm/infrastructure/index.js",
      "require": "./dist/cjs/infrastructure/index.js"
    }
  },
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "vitest run --typecheck.only",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm exec eslint src tests --ext .ts",
    "format": "pnpm exec prettier --write src tests",
    "format:check": "pnpm exec prettier --check src tests"
  },
  "dependencies": {
    "effect": "^3.19.2",
    "plaid": "^39.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@vitest/ui": "^4.0.7",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  },
  "peerDependencies": {
    "effect": "^3.0.0"
  }
}
